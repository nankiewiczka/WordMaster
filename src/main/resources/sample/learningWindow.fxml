<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" style="-fx-background-color: #80bfff;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.LearningWindowController">
   <children>
      <Button fx:id="okButton" layoutX="107.0" layoutY="213.0" mnemonicParsing="false" onAction="#confirmWord" style="-fx-background-color: #cce6ff;" text="OK">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
      <Button fx:id="nextButton" layoutX="191.0" layoutY="213.0" mnemonicParsing="false" onAction="#switchNextWord" style="-fx-background-color: #cce6ff;" text="NEXT">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
      <Button fx:id="endButton" layoutX="311.0" layoutY="153.0" mnemonicParsing="false" onAction="#quitLearning" style="-fx-background-color: #cce6ff;" text="END">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
      <Label fx:id="progressLabel" layoutX="288.0" layoutY="71.0" text="Label" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font></Label>
      <Label fx:id="wordToGuess" layoutX="126.0" layoutY="100.0" text="Label">
         <font>
            <Font size="24.0" />
         </font></Label>
      <TextField fx:id="inputWord" layoutX="76.0" layoutY="153.0" style="-fx-background-color: #cce6ff;">
         <font>
            <Font size="14.0" />
         </font></TextField>
   </children>
</AnchorPane>
