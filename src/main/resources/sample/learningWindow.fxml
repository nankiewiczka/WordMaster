      <Label fx:id="progressLabel" layoutX="387.0" layoutY="111.0" text="Label" />
      <Label fx:id="pointLabel" layoutX="387.0" layoutY="150.0" text="Label" />
      <Label fx:id="wordToGuess" layoutX="132.0" layoutY="127.0" text="Label" />
      <TextField fx:id="inputWord" layoutX="95.0" layoutY="200.0" />
   </children>
</AnchorPane>
